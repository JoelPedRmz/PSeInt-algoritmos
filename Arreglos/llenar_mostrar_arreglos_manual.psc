//Llenar y mostrar un arreglo de forma manual 

Proceso arreglos
	Definir i como entero;
	
	Definir num como entero;
	Dimension num[4];
	
	//Llenar manualmente el arreglo
	num[0] <- 14;
	num[1] <- 18;
	num[2] <- 9;
	num[3] <- 2;
	
	//Mostrar los elementos del arreglo
	Para i<-0 Hasta 3 Con Paso 1 Hacer //Inicia en cero igual que el  arreglo
		Escribir num[i];
	FinPara
FinProceso
