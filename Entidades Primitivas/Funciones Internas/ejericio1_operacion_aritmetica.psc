Proceso principal
	Definir a,b,c,resultado como reales;
	
	Escribir "Ingrese el valor A: ";
	Leer a;
	Escribir "Ingrese el valor B: ";
	Leer b;
	Escribir "Ingrese el valor C: ";
	Leer c;
	
	resultado <- (-b + raiz(b^2 - 4*a*c))/(2*a);
	
	Escribir "El resultado es: ",resultado;
	
FinProceso
